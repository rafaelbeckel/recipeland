{
    "name": "HelloFresh Recipeland",
    "description": "Your source for the yummest recipes!",
    "keywords": ["recipes", "food", "hellofresh"],
    "license": "WTFPL",
    "authors": [
        {
            "name": "Rafael Beckel",
            "email": "rafaelbeckel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.14",
        "filp/whoops": "^2.1",
        "vlucas/phpdotenv": "^2.4",
        "nikic/fast-route": "^1.3",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "psr/cache": "^1.0",
        "monolog/monolog": "^1.23",
        "league/oauth2-server": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "robmorgan/phinx": "^0.9.2",
        "illuminate/database": "^5.6",
        "illuminate/pagination": "^5.6",
        "cheprasov/php-redis-client": "^1.7",
        "ethanhann/redisearch-php": "^0.9.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-issue-3031_fix_based_on_7.0.2",
        "mockery/mockery": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rafaelbeckel/phpunit"
        }
    ],
    "autoload": {
        "psr-4": {
            "Recipeland\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts" : {
        "test" : "./vendor/bin/phpunit"
    }
    
}
